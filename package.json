{
  "name": "docker-dashboard",
  "version": "1.0.0",
  "description": "a web dashboard for Docker",
  "main": "server/index.js",
  "scripts": {
    "build:dev:once": "webpack --mode=development",
    "build:dev": "webpack --mode=development --watch",
    "build:prod": "webpack --mode=production",
    "server:dev": "nodemon",
    "preserver:prod": "tsc -p tsconfig.server.ts",
    "server:prod": "node dist/index.js",
    "start": "concurrently \"npm run server:dev\" \"npm run build:dev\"",
    "production": "npm run build && npm run server",
    "clean": "npx rimraf dist/**/* public/bundle.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/docker-dashboard.git"
  },
  "keywords": [
    "docker",
    "dashboard",
    "react"
  ],
  "author": "Yash Kulshrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/docker-dashboard/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/docker-dashboard#readme",
  "dependencies": {
    "@material-ui/core": "^3.7.1",
    "@material-ui/icons": "^3.0.1",
    "@types/dockerode": "^2.5.10",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.119",
    "@types/material-ui": "^0.21.5",
    "@types/morgan": "^1.7.32",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.29",
    "@types/yup": "^0.26.3",
    "chalk": "^2.4.1",
    "concurrently": "^4.1.0",
    "dockerode": "^2.4.1",
    "express": "^4.15.2",
    "formik": "^1.4.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "socket.io": "^2.2.0",
    "ts-node": "^7.0.1",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.2",
    "react-hot-loader": "^4.6.3",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.3.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3"
  }
}
