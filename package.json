{
  "name": "docker-dashboard",
  "version": "1.0.0",
  "description": "a web dashboard for Docker",
  "main": "server/index.js",
  "scripts": {
    "build:dev": "webpack --mode=development --watch",
    "build:prod": "webpack --mode=production",
    "server:dev": "nodemon/server/index.js",
    "server:prod": "node server/index.js",
    "start": "concurrently \"nodemon server/index.js\" \"npm run build:dev\"",
    "production": "npm run build && npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/docker-dashboard.git"
  },
  "keywords": [
    "docker",
    "dashboard",
    "react"
  ],
  "author": "Yash Kulshrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/docker-dashboard/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/docker-dashboard#readme",
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.54",
    "@types/material-ui": "^0.21.5",
    "@types/morgan": "^1.7.32",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/socket.io": "^2.1.0",
    "@types/socket.io-client": "^1.4.29",
    "@types/yup": "^0.26.2",
    "chalk": "^2.4.1",
    "concurrently": "^4.1.0",
    "dockerode": "^2.4.1",
    "express": "^4.15.2",
    "formik": "^1.4.0",
    "lodash": "^4.17.4",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "socket.io": "^2.2.0",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
