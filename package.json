{
  "name": "docker-dashboard",
  "version": "1.0.0",
  "description": "a web dashboard for Docker",
  "main": "server/index.js",
  "scripts": {
    "build:dev": "webpack --mode=development --watch",
    "build:prod": "webpack --mode=production",
    "server:dev": "NODE_ENV=development nodemon",
    "preserver:prod": "tsc -p tsconfig.server.ts",
    "server:prod": "NODE_ENV=production node dist/index.js",
    "start": "npm run server:dev",
    "production": "npm run build && npm run server:prod",
    "clean": "npx rimraf dist/**/* public/bundle.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/docker-dashboard.git"
  },
  "keywords": [
    "docker",
    "dashboard",
    "react"
  ],
  "author": "Yash Kulshrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/docker-dashboard/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/docker-dashboard#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/dockerode": "^3.2.0",
    "@types/express": "^4.17.8",
    "@types/lodash": "^4.14.163",
    "@types/material-ui": "^0.21.8",
    "@types/morgan": "^1.9.2",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/socket.io": "^2.1.11",
    "@types/socket.io-client": "^1.4.34",
    "@types/yup": "^0.29.9",
    "chalk": "^4.1.0",
    "dockerode": "^3.2.1",
    "express": "^4.17.1",
    "formik": "^2.2.1",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "socket.io": "^2.3.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@types/webpack": "^4.41.24",
    "@types/webpack-dev-middleware": "^3.7.2",
    "@types/webpack-hot-middleware": "^2.25.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "react-hot-loader": "^4.13.0",
    "source-map-loader": "^1.1.2",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.3.1",
    "webpack-cli": "^4.1.0",
    "webpack-dev-middleware": "^4.0.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0"
