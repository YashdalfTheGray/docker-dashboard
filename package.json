{
  "name": "docker-dashboard",
  "version": "1.0.0",
  "description": "a web dashboard for Docker",
  "main": "server/index.js",
  "scripts": {
    "build:dev": "webpack --mode=development --watch",
    "build:prod": "webpack --mode=production",
    "server:dev": "NODE_ENV=development nodemon",
    "preserver:prod": "tsc -p tsconfig.server.ts",
    "server:prod": "NODE_ENV=production node dist/index.js",
    "start": "npm run server:dev",
    "production": "npm run build && npm run server:prod",
    "clean": "npx rimraf dist/**/* public/bundle.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/docker-dashboard.git"
  },
  "keywords": [
    "docker",
    "dashboard",
    "react"
  ],
  "author": "Yash Kulshrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/docker-dashboard/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/docker-dashboard#readme",
  "dependencies": {
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@types/dockerode": "^2.5.21",
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.149",
    "@types/material-ui": "^0.21.7",
    "@types/morgan": "^1.7.37",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.29",
    "@types/yup": "^0.26.26",
    "chalk": "^3.0.0",
    "dockerode": "^3.0.2",
    "express": "^4.17.1",
    "formik": "^2.0.8",
    "lodash": "^4.17.19",
    "morgan": "^1.8.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "socket.io": "^2.3.0",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/webpack-hot-middleware": "^2.25.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.18",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.0",
    "webpack-hot-middleware": "^2.25.0"
