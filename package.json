{
  "name": "docker-dashboard",
  "version": "1.0.0",
  "description": "a web dashboard for Docker",
  "main": "server/index.js",
  "scripts": {
    "build:dev": "webpack --mode=development --watch",
    "build:prod": "webpack --mode=production",
    "server:dev": "NODE_ENV=development nodemon",
    "preserver:prod": "tsc -p tsconfig.server.ts",
    "server:prod": "NODE_ENV=production node dist/index.js",
    "start": "npm run server:dev",
    "production": "npm run build && npm run server:prod",
    "clean": "npx rimraf dist/**/* public/bundle.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/docker-dashboard.git"
  },
  "keywords": [
    "docker",
    "dashboard",
    "react"
  ],
  "author": "Yash Kulshrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/docker-dashboard/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/docker-dashboard#readme",
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@types/dockerode": "^2.5.20",
    "@types/express": "^4.17.0",
    "@types/lodash": "^4.14.136",
    "@types/material-ui": "^0.21.6",
    "@types/morgan": "^1.7.36",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.29",
    "@types/yup": "^0.26.22",
    "chalk": "^2.4.2",
    "dockerode": "^2.5.8",
    "express": "^4.17.1",
    "formik": "^1.5.8",
    "lodash": "^4.17.15",
    "morgan": "^1.8.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "socket.io": "^2.2.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/webpack": "^4.32.1",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/webpack-hot-middleware": "^2.16.5",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.9",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
