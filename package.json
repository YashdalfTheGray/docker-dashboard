{
  "name": "docker-dashboard",
  "version": "1.0.0",
  "description": "a web dashboard for Docker",
  "main": "server/index.js",
  "scripts": {
    "build:dev": "webpack --mode=development --progress",
    "build:watch": "webpack --mode=development --watch --progress",
    "build:prod": "webpack --mode=production --progress",
    "server:dev": "NODE_ENV=development nodemon",
    "preserver:prod": "tsc -p tsconfig.server.ts",
    "server:prod": "NODE_ENV=production node dist/index.js",
    "start": "npm run server:dev",
    "production": "npm run build && npm run server:prod",
    "clean": "npx rimraf dist/**/* public/bundle.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/docker-dashboard.git"
  },
  "keywords": [
    "docker",
    "dashboard",
    "react"
  ],
  "author": "Yash Kulshrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/docker-dashboard/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/docker-dashboard#readme",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@types/dockerode": "^3.3.19",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.14.201",
    "@types/material-ui": "^0.21.12",
    "@types/morgan": "^1.9.5",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/yup": "^0.29.9",
    "chalk": "^5.3.0",
    "dockerode": "^3.3.5",
    "express": "^4.21.2",
    "formik": "^2.4.5",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.7.2",
    "yup": "^1.3.2"
  },
